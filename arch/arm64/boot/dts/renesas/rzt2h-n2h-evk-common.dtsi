/ {
	aliases {
		i2c0 = &i2c0;
		i2c1 = &i2c1;
		serial0 = &sci0;
	};

	chosen {
		stdout-path = "serial0:115200n8";
	};
};

&extal_clk {
	clock-frequency = <25000000>;
};

&sci0 {
	status = "okay";
};
